function test {
    echo "____________"
    echo ""
    list="$1"
    sed "s|$rooms_path|#> |g" <<<"$list"
    exeiac debug -debug-command="convert_to_elementary_bricks_path '$list'"
    echo "\$?:$?"
}

test "$list_1"
test "$list_2"
test "$list_3"
test "$list_4"

